{
    "name": "remark-itinerary",
    "version": "0.2.2",
    "private": false,
    "type": "module",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./utils": {
            "types": "./dist/utils.d.ts",
            "import": "./dist/utils.js"
        }
    },
    "sideEffects": false,
    "engines": {
        "node": ">=18"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cumuloworks/itinerary-md.git",
        "directory": "packages/core"
    },
    "bugs": {
        "url": "https://github.com/cumuloworks/itinerary-md/issues"
    },
    "homepage": "https://github.com/cumuloworks/itinerary-md#readme",
    "keywords": [
        "markdown",
        "remark",
        "mdast",
        "itinerary",
        "travel",
        "parser",
        "plugin"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build": "rimraf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
        "dev": "tsc -w -p tsconfig.build.json",
        "typecheck": "tsc -p tsconfig.build.json --noEmit",
        "prepack": "npm run build",
        "test": "vitest --run --passWithNoTests",
        "test:watch": "vitest"
    },
    "dependencies": {
        "luxon": "^3.7.1",
        "mdast-util-to-string": "4.0.0",
        "unist-util-visit": "^5.0.0",
        "expr-eval": "^2.0.2"
    },
    "peerDependencies": {
        "unified": "^11.0.0"
    },
    "devDependencies": {
        "@types/node": "^24.3.1",
        "remark-parse": "^11.0.0",
        "unified": "^11.0.5",
        "typescript": "^5.9.2",
        "rimraf": "^6.0.1",
        "vitest": "^3.2.4"
    }
}
